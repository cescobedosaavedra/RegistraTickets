<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RegistrodeTickets"
                   namespace="RegistrodeTickets.Dominio"
                   default-lazy="false">
  <class name="GuardaTicket" table="t_tickets">
    <id name="Id" column="id">
      <generator class="increment" />
    </id>
    <property name="CodEmpleado" column="codEmpleado" />
    <property name="N_ticket" column="n_ticket" />
    <property name="F_creacion" column="f_creacion" />
    <property name="Estado" column="estado" />
    <property name="Prioridad" column="prioridad" />
    <property name="NombreEmpleado" column="NombreEmpleado" />
    <property name="ApellidoEmpleado" column="ApellidoEmpleado" />
    <property name="CorreoEmpleado" column="CorreoEmpleado" />
    <property name="CargoEmpleado" column="CargoEmpleado" />
    <property name="TelefonoEmpleado" column="TelefonoEmpleado" />
    <property name="AreaEmpleado" column="AreaEmpleado" />
    <property name="AsuntoTicket" column="asuntoTicket" />
    <property name="ObservacionTicket" column="observacionTicket" />
  
        
  </class>
</hibernate-mapping>
